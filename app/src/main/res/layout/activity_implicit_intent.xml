<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_implicit_intent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.uniclass.sangji.uniclassandroid.ImplicitIntent">

    <Button
        android:id="@+id/implicit_call"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="implicitCallOnClick"
        android:text="전화걸기"
        />

    <Button
        android:id="@+id/implicit_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="implicitCallOnClick"
        android:text="지도보기"
        />

    <Button
        android:id="@+id/implicit_web"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="implicitCallOnClick"
        android:text="웹브라우저"
        />

    <Button
        android:id="@+id/implicit_contact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="implicitCallOnClick"
        android:text="연락처보기"
        />

</LinearLayout>
